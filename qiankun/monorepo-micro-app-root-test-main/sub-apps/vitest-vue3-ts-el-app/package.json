		"element-plus": "^2.4.4",
		"jsencrypt": "^3.3.2",
		"lodash-es": "^4.17.21",
		"mockjs": "^1.1.0",
		"nprogress": "^0.2.0",
		"pinia": "^2.1.7",
		"qs": "^6.11.2",
		"vue": "^3.3.11",
		"vue-i18n": "^9.8.0",
