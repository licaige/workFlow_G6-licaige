{
	"name": "vite-react-ts-jest-app",
	"private": true,
	"version": "0.0.0",
	"description": "vite 构建react+ts+jest pc端微应用模版",
	"type": "module",
	"scripts": {
		"dev": "vite --config ./viter/vite.config.ts",
		"dev:local": "pnpm dev --mode _local",
		"testing": "pnpm dev --mode testing",
		"staging": "pnpm dev --mode staging",
		"build:testing": "pnpm build-only --mode testing && pnpm type-check",
		"build:staging": "pnpm build-only --mode staging && pnpm tsc-only",
		"build:prod": "pnpm build-only --mode production && pnpm tsc-prod",
		"build-only": "vite build --config ./viter/vite.config.ts",
		"tsc-prod": "tsc -p ./tsconfig.prod.json",
		"tsc-only": "tsc --noEmit",
		"type-check": "tsc --noEmit -p tsconfig.test.json --composite false",
		"preview": "vite preview --config ./viter/vite.config.ts",
		"test:unit": "jest",
		"test:watch": "jest --env=testing --watch",
		"test:nc": "pnpm test:watch --no-cache",
		"format": "prettier --write --ignore-unknown .",
		"lint": "eslint --cache . --ext .js,.jsx,ts,tsx --report-unused-disable-directives --max-warnings 0 --fix --ignore-path .gitignore",
		"lint-staged": "lint-staged",
		"analyze": "source-map-explorer 'dist/assets/*.js'"
	},
	"dependencies": {
		"@micro-zoe/micro-app": "1.0.0-rc.3",
		"antd": "^5.12.5",
		"axios": "^1.6.3",
		"dayjs": "^1.11.10",
		"echarts": "^5.4.3",
		"echarts-for-react": "^3.0.2",
		"immer": "^10.0.3",
		"lodash-es": "^4.17.21",
		"lru-cache": "^10.1.0",
		"qs": "^6.11.2",
		"react": "^18.2.0",
		"react-dnd": "^16.0.1",
		"react-dnd-html5-backend": "^16.0.1",
		"react-dom": "^18.2.0",
		"react-helmet-async": "^2.0.4",
		"react-router-dom": "^6.21.1",
		"use-immer": "^0.9.0"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^6.1.6",
		"@testing-library/react": "^14.1.2",
		"@testing-library/react-hooks": "^8.0.1",
		"@testing-library/user-event": "^14.5.2",
		"@types/jest": "^29.5.11",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^20.10.5",
		"@types/react": "^18.2.43",
		"@types/react-dom": "^18.2.17",
		"@types/react-router-dom": "^5.3.3",
		"@typescript-eslint/eslint-plugin": "^6.14.0",
		"@typescript-eslint/parser": "^6.14.0",
		"@vitejs/plugin-basic-ssl": "^1.1.0",
		"@vitejs/plugin-legacy": "^5.2.0",
		"@vitejs/plugin-react": "^4.2.1",
		"autoprefixer": "^10.4.16",
		"babel-jest": "^29.7.0",
		"cssnano": "^6.0.2",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-prettier": "^5.1.2",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"jest-watch-typeahead": "^2.2.2",
		"less": "^4.2.0",
		"less-vars-to-js": "^1.3.0",
		"lint-staged": "^15.2.0",
		"mockjs": "^1.1.0",
		"npm-run-all2": "^6.1.1",
		"postcss": "^8.4.32",
		"postcss-flexbugs-fixes": "^5.0.2",
		"postcss-import": "^15.1.0",
		"postcss-nesting": "^12.0.2",
		"postcss-normalize": "^10.0.1",
		"postcss-preset-env": "^9.3.0",
		"postcss-url": "^10.1.3",
		"prettier": "^3.1.1",
		"rollup-plugin-visualizer": "^5.12.0",
		"sass": "^1.69.6",
		"terser": "^5.28.1",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.2.2",
		"vite": "^5.0.8",
		"vite-plugin-babel-import": "^2.0.5",
		"vite-plugin-cdn2": "^1.1.0",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-html": "^3.2.1",
		"vite-plugin-imp": "^2.4.0",
		"vite-plugin-inspect": "^0.8.1",
		"vite-plugin-mock": "^3.0.0",
		"vite-plugin-svgr": "^4.2.0"
	},
	"browserslist": [
		"> 0.5%",
		"last 2 versions",
		"Firefox ESR",
		"not dead",
		"IE 11",
		"not IE 10"
	],
	"engines": {
		"node": ">= v16.20.0",
		"npm": ">= 8.19.4"
	}
}