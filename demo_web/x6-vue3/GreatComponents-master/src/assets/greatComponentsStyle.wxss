/*------------------   面包屑 START   --------------------*/
.great-bread {
  height: 32px;
  line-height: 32px;
  vertical-align: middle;
}
.great-bread .great-bread-split-with {
  display: inline-block;
  width: 6px;
  height: 6px;
  background: #666666;
  border-radius: 50%;
  margin: 10px 7px 16px 12px;
}
.great-bread .great-bread-root-node {
  vertical-align: top;
  height: 32px;
  line-height: 32px;
  display: inline-block;
  font-size: 14px;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #0229FF;
  letter-spacing: 1px;
  cursor: pointer;
}
.great-bread .great-bread-root-node:hover {
  color: rgba(0, 0, 0, 0.64705882);
}
.great-bread .great-bread-current-node {
  vertical-align: top;
  height: 32px;
  line-height: 32px;
  display: inline-block;
  color: rgba(0, 0, 0, 0.64705882);
  font-size: 14px;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #454369;
  letter-spacing: 1px;
}
/*------------------   面包屑 END   --------------------*/
/*------------------   滚动列表 START  --------------------*/
.scroll-board {
  width: 100%;
  height: 100%;
  overflow: hidden;
  position: relative;
}
.scroll-board .scroll-board-item {
  width: 100%;
  position: absolute;
  left: 0;
  background: #fff;
  white-space: nowrap;
  word-break: break-all;
  overflow: hidden;
}
/*------------------   滚动列表 END  --------------------*/
/*------------------   头导航 START  --------------------*/
.great-header {
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.great-header .great-header-title {
  font-size: 24px;
  height: 74px;
  line-height: 74px;
  display: inline-block;
  padding-left: 20px;
}
.great-header .great-header-list {
  margin: 0;
  position: fixed;
  left: 328px;
  right: 330px;
  top: 30px;
  height: 34px;
  width: calc(-208%);
  display: inline-block;
  box-sizing: border-box;
  overflow: hidden;
}
.great-header .great-header-list .great-header-list-item {
  overflow: hidden;
  transform: rotate(-38deg);
  top: -27px;
  position: relative;
  cursor: pointer;
  list-style: none;
  display: inline-block;
  font-size: 16px;
  height: 85px;
  min-width: 162px;
  background: #F5F8FF;
  border: 1px solid #CDD7F0;
  color: #5B646F;
  margin-left: -24px;
}
.great-header .great-header-list .great-header-list-item:hover,
.great-header .great-header-list .great-header-list-item.active {
  background: #DEE8FF;
  border: 1px solid #6A82FF;
  color: #0229FF;
}
.great-header .great-header-list .great-header-list-item:hover .great-header-list-title,
.great-header .great-header-list .great-header-list-item.active .great-header-list-title {
  background: #DEE8FF;
  border: 1px solid #6A82FF;
  color: #0229FF;
}
.great-header .great-header-list .great-header-list-item .great-header-list-title {
  display: inline-block;
  transform: rotate(38deg);
  height: 32px;
  line-height: 32px;
  text-align: center;
  width: 180px;
  background: #F5F8FF;
  border: 1px solid #CDD7F0;
  color: #5B646F;
  position: relative;
  top: 25px;
  left: -1px;
  font-size: 18px;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  letter-spacing: 2px;
}
/*------------------   头导航 END  --------------------*/
/*------------------   loading遮罩层 START  --------------------*/
.loading-parent--relative {
  position: relative;
}
.loading-mask {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  overflow: hidden;
  background: rgba(0, 0, 0, 0.3);
  z-index: 9;
  display: flex;
  flex-flow: column;
  justify-content: center;
}
.loading-mask .loading-mask-icon {
  display: inline-block;
  width: 100%;
  height: 40px;
  line-height: 40px;
  text-align: center;
  background: url('data:image/png;base64,') no-repeat center center;
  background-size: contain;
  animation: turn 2s linear infinite;
}
@keyframes turn {
  0% {
    -webkit-transform: rotate(0deg);
  }
  25% {
    -webkit-transform: rotate(90deg);
  }
  50% {
    -webkit-transform: rotate(180deg);
  }
  75% {
    -webkit-transform: rotate(270deg);
  }
  100% {
    -webkit-transform: rotate(360deg);
  }
}
.loading-mask.hide {
  display: none;
}
/*------------------   loading遮罩层 END  --------------------*/
/*------------------   滚动条全局样式 START  --------------------*/
::-webkit-scrollbar {
  width: 5px;
  /*no*/
  height: 5px;
  /*no*/
}
::-webkit-scrollbar-track {
  border-radius: 0;
}
::-webkit-scrollbar-thumb {
  background-color: rgba(117, 227, 255, 0.3);
  transition: all 0.2s;
  border-radius: 2px;
}
::-webkit-scrollbar-thumb:hover {
  background-color: #75e3ff;
}
/*------------------   滚动条全局样式 END  --------------------*/
