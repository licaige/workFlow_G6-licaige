{
  "name": "vue3-perfect-scrollbar",
  "version": "1.6.1",
  "description": "Vue.js wrapper for perfect scrollbar",
  "paths": {
    "source": "src",
    "destination": "dist",
    "example": "example"
  },
  "main": "dist/vue3-perfect-scrollbar.cjs.js",
  "module": "dist/vue3-perfect-scrollbar.esm.js",
  "style": "dist/vue3-perfect-scrollbar.css",
  "types": "./types/vue3-perfect-scrollbar.d.ts",
  "scripts": {
    "build": "rollup -c --environment production",
    "build:css": "postcss src/style.css -o dist/vue3-perfect-scrollbar.min.css --env production && postcss src/style.css -o dist/vue3-perfect-scrollbar.css",
    "dev": "webpack-dev-server --config webpack.config.js",
    "test": "yarn jest",
    "test:dev": "node --inspect-brk node_modules/.bin/jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mercs600/vue3-perfect-scrollbar"
  },
  "author": "Adam Marcinkowski <developer@mercs600.pl>",
  "keywords": [
    "frontend",
    "scroll",
    "scrollbar",
    "vue"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@vue/compiler-sfc": "^3.0.2",
    "@vue/test-utils": "^2.0.0-beta.7",
    "babel-loader": "^8.0.2",
    "css-loader": "^1.0.0",
    "eslint": "^7.12.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^7.1.0",
    "eslint-webpack-plugin": "^2.1.0",
    "jest": "^26.6.1",
    "node-sass": "^4.14.1",
    "postcss": "^8.1.4",
    "postcss-cli": "^8.1.0",
    "rollup": "^0.65.2",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-uglify": "^5.0.2",
    "sass-loader": "^10.0.4",
    "style-loader": "^0.23.0",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.3",
    "vue": "^3.0.2",
    "vue-jest": "^5.0.0-alpha.5",
    "vue-loader": "^16.0.0-beta.8",
    "vue-style-loader": "^4.1.2",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7"
  },
  "dependencies": {
    "cssnano": "^5.1.14",
    "perfect-scrollbar": "^1.5.5",
    "postcss-import": "^12.0.0"
  },
  "volta": {
    "node": "12.22.12"
  }
}
